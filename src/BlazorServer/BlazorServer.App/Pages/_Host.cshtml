@page "/"
@namespace BlazorServer.App.Pages ;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>BlazorServer.App</title>
    <base href="~/"/>
    <link href="lib/quill/dist/quill.snow.css" rel="stylesheet"/>
    <link href="lib/quill/dist/quill.bubble.css" rel="stylesheet"/>
    <link href="lib/toastr/build/toastr.min.css" rel="stylesheet"/>
    <link href="lib/open-iconic/font/css/open-iconic-bootstrap.min.css" rel="stylesheet"/>
    <link href="lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet"/>
    <link href="css/site.css" rel="stylesheet"/>
    <link href="BlazorServer.App.styles.css" rel="stylesheet"/>
</head>
<body>
<component type="typeof(App)" render-mode="ServerPrerendered"/>

<div id="blazor-error-ui">
    <environment include="Staging,Production">
        An error has occurred. This application may no longer respond until reloaded.
    </environment>
    <environment include="Development">
        An unhandled exception has occurred. See browser dev tools for details.
    </environment>
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>

<script src="lib/jquery/dist/jquery.min.js"></script>
<script src="lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="lib/toastr/build/toastr.min.js"></script>
<script src="lib/quill/dist/quill.min.js"></script>
<script src="_content/Blazored.TextEditor/quill-blot-formatter.min.js"></script>
<script src="_content/Blazored.TextEditor/Blazored-BlazorQuill.js"></script>
<script src="js/common.js"></script>
<script src="_framework/blazor.server.js"></script>
</body>
</html>